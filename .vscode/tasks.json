{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "typescript",
			"tsconfig": "typescript/tsconfig.json",
			"problemMatcher": [
				"$tsc"
			],
			"group": "build",
			"label": "Compile"
		},
		{
			"type": "shell",
			"group": "build",
			"command": "cd '${workspaceFolder}/typescript/src'&&cat ../js/utilities.js ../js/web-scheme.js ../js/web-scheme-file.js ../js/web-scheme-web.js ../js/web-fetch-store.js web-view.js web-application.js > _app.js",
			"label": "Package",
			"problemMatcher": []
		},
		{
			"type": "shell",
			"group": "build",
			"command": "cd '${workspaceFolder}/typescript/src'&&cat ../js/client/keyboard.js ../js/client/environment.js ../js/client/app.js > ../js/client/_client.js",
			"label": "Package Client",
			"problemMatcher": []
		},
		{
			"label": "Build All",
			"type": "shell",
			"command": "echo Build Completed ",
			"dependsOrder": "sequence",
			"dependsOn": [
				"Compile",
				"Package",
				"Package Client"
			],
			"problemMatcher": [],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}